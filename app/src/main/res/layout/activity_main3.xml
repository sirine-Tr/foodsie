<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity3">

    <Button
        android:id="@+id/Add"
        android:layout_width="147dp"
        android:layout_height="55dp"
        android:layout_marginStart="132dp"
        android:layout_marginLeft="132dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="132dp"
        android:layout_marginRight="132dp"
        android:text="add"
        app:backgroundTint="#0B0B0B"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tt4" />

    <EditText
        android:id="@+id/tt1"
        android:layout_width="286dp"
        android:layout_height="59dp"
        android:layout_marginStart="62dp"
        android:layout_marginLeft="62dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="63dp"
        android:layout_marginRight="63dp"
        android:layout_marginBottom="45dp"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPersonName"

        android:hint="@string/restaurant_name"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColorHint="#565E5B5B"
        android:textColor="@color/black"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/tt2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="visible" />

    <EditText
        android:id="@+id/tt2"
        android:layout_width="282dp"
        android:layout_height="65dp"
        android:layout_marginStart="65dp"
        android:layout_marginLeft="65dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="64dp"
        android:layout_marginRight="64dp"
        android:layout_marginBottom="46dp"
        android:ems="10"
        android:gravity="center"

        android:inputType="phone"
        android:hint="phone_number"
        android:textColorHint="#565E5B5B"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/tt3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tt1"
        tools:ignore="HardcodedText" />

    <EditText
        android:id="@+id/tt3"
        android:layout_width="285dp"
        android:layout_height="70dp"
        android:layout_marginStart="62dp"
        android:layout_marginLeft="62dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="64dp"
        android:layout_marginRight="64dp"
        android:layout_marginBottom="59dp"
        android:ems="10"
        android:gravity="center"
        android:inputType="numberDecimal"

        android:hint="@string/total_money"
        android:textColorHint="#565E5B5B"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/tt4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tt2"
        tools:ignore="HardcodedText" />

    <EditText
        android:id="@+id/tt4"
        android:layout_width="280dp"
        android:layout_height="73dp"
        android:layout_marginStart="67dp"
        android:layout_marginLeft="67dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="64dp"
        android:layout_marginRight="64dp"
        android:layout_marginBottom="80dp"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPersonName"
        android:hint="Location"
        android:textColorHint="#565E5B5B"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/Add"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tt3" />




</androidx.constraintlayout.widget.ConstraintLayout>